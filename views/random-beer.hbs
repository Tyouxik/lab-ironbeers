<h1>Random Beers</h1>
{{#each randomBeer}}
<img src="{{this.image_url}} " alt="{{this.name}}">
<h1>{{this.name}}</h1>
<p>Description: {{this.description}} </p>
<p>{{this.tagline}}</p>
{{#each food_pairing}}
<ul>
    <li>{{this}}</li>
</ul>
{{/each}}
<p>Brewer's tips: {{this.brewers_tips}}</p>
{{/each}}
